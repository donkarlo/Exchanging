<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ndrm\EntityTestsBundle\Entity\UsersRoles" table="users_roles">
    <indexes>
      <index name="id_users" columns="id_users"/>
      <index name="id_roles" columns="id_roles"/>
    </indexes>
    <id name="idRoles" type="integer" column="id_roles"/>
    <id name="idUsers" association-key="true"/>
    <one-to-one field="idUsers" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="id_users" referenced-column-name="id_users"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
