<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ndrm\EntityTestsBundle\Entity\Users" table="users">
    <unique-constraints>
      <unique-constraint name="UNIQ_1483A5E9F85E0677" columns="username"/>
      <unique-constraint name="UNIQ_1483A5E9E7927C74" columns="email"/>
      <unique-constraint name="UNIQ_1483A5E97B495A69" columns="cell_phone"/>
    </unique-constraints>
    <id name="idUsers" type="integer" column="id_users">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
    <field name="cellPhone" type="string" column="cell_phone" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
